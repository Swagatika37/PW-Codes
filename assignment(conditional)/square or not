#include<iostream>
using namespace std;

int main (){
    int length;
    cout<<"Enter length : ";
    cin>>length;

    int breadth;
    cout<<"Enter breadth : ";
    cin>>breadth;

    if (length==breadth)
    {
      cout<<"square"<<endl;
    }
    else {
      cout<<"rectangle"<<endl;
    }

  return 0;

   }
